<body class="hidden">
    <main>
        <div class="cd-index cd-main-content">
            <!-- Page Content -->
            <div id="page-content" class="" data-bgcolor="#fff" data-textcolor="#000">
                <div class="page-overlay from-bottom">
                    <div class="animate-box-bottom"></div>
                    <div class="animate-box-left"></div>
                </div>

                <!-- Header -->
                <header class="scroll-hide">
                    <div id="header-container">

                        <!-- Logo -->
                        <div id="logo">
                            <a class="ajax-link" data-type="page-transition" asp-controller="Home" asp-action="Index">
                                <img class="black-logo" src="~/images/logo.svg" alt="Logo Black">
                                <img class="white-logo" src="~/images/logo.svg" alt="Logo White">
                            </a>
                        </div>
                        <!--/Logo -->

                        <a asp-controller="Home" asp-action="Index" class="menu-info">دسته بندی</a>

                        <!--/Menu Burger Dots-->
                        <!-- Navigation -->
                        <nav>
                            <ul data-breakpoint="1024" class="flexnav">
                                <li><a class="ajax-link" href="about.html" data-type="page-transition">تاریخچه</a></li>
                                <li><a id="menu-burger-dots">دسته بندی</a></li>
                                <li><a class="ajax-link" asp-controller="Home" asp-action="Contact" data-type="page-transition">ارتباط با من</a></li>
                            </ul>
                        </nav>
                        <!--/Navigation -->
                        <!-- Menu Burger -->
                        <div id="menu-burger">
                            <span></span>
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                        <!--/Menu Burger -->

                    </div>
                </header>
                <!--/Header -->
                @RenderBody();
            </div>
        </div>
</main>


    <div class="cd-cover-layer"></div>

    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/plugins.js"></script>
    <script src="~/js/scripts.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>